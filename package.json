    "test-linter": "pnpm nx run-many --all --target=test-linter",
    "test-type": "pnpm nx run-many --all --target=test-type",
    "test-unit": "pnpm nx run-many --all --target=test-unit"
  },
  "workspaces": [
    "demo/*",
    "cdk-dynamodb-migrator"
