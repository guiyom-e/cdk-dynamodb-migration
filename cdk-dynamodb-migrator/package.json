{
  "name": "cdk-dynamodb-migrator",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "dependencies": {
    "aws-cdk-lib": "2.86.0",
    "constructs": "^10.2.70",
    "esbuild": "^0.18.20",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "20.1.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "aws-cdk": "2.86.0",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "jsii": {
    "versionFormat": "full",
    "targets": {
      "java": {
        "package": "aws.dynamodb.migrator",
        "maven": {
          "groupId": "aws.dynamodb.migrator",
          "artifactId": "core"
        }
      },
      "python": {
        "distName": "cdk-dynamodb-migrator",
        "module": "cdk-dynamodb-migrator"
      },
      "dotnet": {
        "namespace": "Dynamodb.Migrator",
        "packageId": "Dynamodb.Migrator"
      }
    },
    "tsc": {
      "outDir": "build",
      "rootDir": "."
    },
    "excludeTypescript": [
      "test/**/*.ts"
    ]
  },
  "main": "./build/index.js",
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "test": "jest",
    "watch": "tsc -w"
  },
  "stability": "experimental",
  "types": "./build/index.d.ts"
}
