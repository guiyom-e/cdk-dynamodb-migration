{
  "name": "cdk-dynamodb-migrator",
  "description": "CDK lib to manage migrations",
  "version": "0.1.0",
  "author": {
    "name": "Guillaume Égée"
  },
  "contributors": [
    {
      "name": "Zineb El Bachiri"
    }
  ],
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "dependencies": {
    "aws-cdk-lib": "2.99.1",
    "constructs": "^10.2.70"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.5.9",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "aws-cdk": "2.94.0",
    "esbuild": "^0.19.2",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "jsii": "^5.2.11",
    "jsii-pacmak": "^1.88.0",
    "prettier": "3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "jsii": {
    "versionFormat": "full",
    "targets": {
      "java": {
        "package": "aws.dynamodb.migrator",
        "maven": {
          "groupId": "aws.dynamodb.migrator",
          "artifactId": "core"
        }
      },
      "python": {
        "distName": "cdk-dynamodb-migrator",
        "module": "cdk-dynamodb-migrator"
      },
      "dotnet": {
        "namespace": "Dynamodb.Migrator",
        "packageId": "Dynamodb.Migrator"
      }
    },
    "tsc": {
      "outDir": "build",
      "rootDir": "."
    },
    "excludeTypescript": [
      "test/**/*.ts",
      "bin/**/*.ts"
    ]
  },
  "license": "MIT",
  "main": "./build/index.js",
  "repository": "guiyom-e/cdk-dynamodb-migration",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii --watch",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "package": "jsii-pacmak",
    "test": "pnpm test-linter && pnpm test-type && pnpm test-unit",
    "test-linter": "eslint .",
    "test-type": "tsc --noEmit",
    "test-unit": "jest",
    "watch": "tsc -w"
  },
  "stability": "experimental",
  "types": "./build/index.d.ts"
}
